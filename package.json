{
  "name": "base-converter",
  "version": "1.0.0",
  "description": "A simple base converter library",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "author": "Nati Kamusher <natan.kamusher@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "check-types": "tsc --noEmit",
