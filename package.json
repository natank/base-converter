    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "serve": "^14.2.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.6",
